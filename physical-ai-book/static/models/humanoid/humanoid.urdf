  <link name="r_hand">
    <visual>
      <geometry>
        <box size="0.08 0.05 0.12"/>
      </geometry>
      <origin xyz="0 0 -0.06"/>
      <material name="orange"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.05 0.12"/>
      </geometry>
      <origin xyz="0 0 -0.06"/>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <origin xyz="0 0 -0.06"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
