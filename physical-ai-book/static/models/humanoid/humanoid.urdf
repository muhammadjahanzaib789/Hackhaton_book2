<?xml version="1.0"?>
<!--
  Physical AI Book - 21-DOF Humanoid Robot URDF

  Joint configuration:
  - Torso: 1 DOF (waist rotation)
  - Each Arm: 7 DOF (shoulder_pitch, shoulder_roll, shoulder_yaw, elbow, wrist_pitch, wrist_roll, wrist_yaw)
  - Each Leg: 6 DOF (hip_pitch, hip_roll, hip_yaw, knee, ankle_pitch, ankle_roll)

  Total: 1 + 7*2 + 6*2 = 21 DOF
-->
<robot name="humanoid">

  <!-- ==================== MATERIALS ==================== -->
  <material name="grey">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>
  <material name="blue">
    <color rgba="0.2 0.4 0.8 1"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.5 0.0 1"/>
  </material>
  <material name="white">
    <color rgba="0.9 0.9 0.9 1"/>
  </material>

  <!-- ==================== BASE LINK (PELVIS) ==================== -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.25 0.3 0.15"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.3 0.15"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
    </inertial>
  </link>

  <!-- ==================== TORSO ==================== -->
  <joint name="waist_yaw" type="revolute">
    <parent link="base_link"/>
    <child link="torso"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="100" velocity="2"/>
  </joint>

  <link name="torso">
    <visual>
      <geometry>
        <box size="0.3 0.35 0.4"/>
      </geometry>
      <origin xyz="0 0 0.2"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.3 0.35 0.4"/>
      </geometry>
      <origin xyz="0 0 0.2"/>
    </collision>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0 0 0.2"/>
      <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <!-- ==================== HEAD ==================== -->
  <joint name="neck_yaw" type="revolute">
    <parent link="torso"/>
    <child link="head"/>
    <origin xyz="0 0 0.45" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="20" velocity="3"/>
  </joint>

  <link name="head">
    <visual>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
      <origin xyz="0 0 0.1"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
      <origin xyz="0 0 0.1"/>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <origin xyz="0 0 0.1"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Camera sensor on head -->
  <joint name="camera_joint" type="fixed">
    <parent link="head"/>
    <child link="camera_link"/>
    <origin xyz="0.1 0 0.1" rpy="0 0 0"/>
  </joint>

  <link name="camera_link">
    <visual>
      <geometry>
        <box size="0.02 0.05 0.02"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>

  <!-- ==================== RIGHT ARM (7 DOF) ==================== -->
  <!-- Shoulder -->
  <joint name="r_shoulder_pitch" type="revolute">
    <parent link="torso"/>
    <child link="r_upper_arm_pitch"/>
    <origin xyz="0 -0.2 0.35" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="2"/>
  </joint>

  <link name="r_upper_arm_pitch">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="r_shoulder_roll" type="revolute">
    <parent link="r_upper_arm_pitch"/>
    <child link="r_upper_arm_roll"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="50" velocity="2"/>
  </joint>

  <link name="r_upper_arm_roll">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="r_shoulder_yaw" type="revolute">
    <parent link="r_upper_arm_roll"/>
    <child link="r_upper_arm"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="50" velocity="2"/>
  </joint>

  <link name="r_upper_arm">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.25"/>
      </geometry>
      <origin xyz="0 0 -0.125" rpy="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.04" length="0.25"/>
      </geometry>
      <origin xyz="0 0 -0.125" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="1.5"/>
      <origin xyz="0 0 -0.125"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <!-- Elbow -->
  <joint name="r_elbow" type="revolute">
    <parent link="r_upper_arm"/>
    <child link="r_lower_arm"/>
    <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="2.5" effort="30" velocity="2"/>
  </joint>

  <link name="r_lower_arm">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.22"/>
      </geometry>
      <origin xyz="0 0 -0.11" rpy="0 0 0"/>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.035" length="0.22"/>
      </geometry>
      <origin xyz="0 0 -0.11" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 -0.11"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <!-- Wrist -->
  <joint name="r_wrist_pitch" type="revolute">
    <parent link="r_lower_arm"/>
    <child link="r_wrist_pitch_link"/>
    <origin xyz="0 0 -0.22" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="3"/>
  </joint>

  <link name="r_wrist_pitch_link">
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="r_wrist_roll" type="revolute">
    <parent link="r_wrist_pitch_link"/>
    <child link="r_wrist_roll_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="3"/>
  </joint>

  <link name="r_wrist_roll_link">
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
