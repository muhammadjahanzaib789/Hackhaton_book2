  </link>

  <joint name="r_wrist_yaw" type="revolute">
    <parent link="r_wrist_roll_link"/>
    <child link="r_hand"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
